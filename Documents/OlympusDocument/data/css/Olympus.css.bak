/* navigation menu */
/*****************************************************/

nav 
{ 
	margin:0px 0px; 
	width:1000px;
	border-top: 1px dashed #63AFC4;
}

nav ul 
{
	padding:0;
	margin:0;
	list-style: none;
	position: relative;
	display:block;
}
	
nav ul li 
{
	float:left;
	display:list-item;
	list-style: none; 	
	background-color:#63AFC4;
	width: 200px;
	height: 35px;
}

nav ul li a 
{
	display:block;
	padding-top: 7px;
	padding-bottom: 7px;
	padding-right: 5px;
	padding-left: 5px;	
	color:#FFFFFF;
	font-size:13px;
	text-decoration:none;
}

nav li a:hover 
{ 
/*	background-color: #FF979D;*/
	background-color: #F8FBFC;
	color: #63AFC4;
/*	border-bottom: 1px dashed #FF979D; */
/*	border-top: 1px dashed #FF979D; */
/*	border-left: 1px dashed #FF979D; */
	border-right: 10px solid #FF979D;
	font-size:13px;
/*	font-weight: bold;*/
}

nav li > a:after 
{ 
	content: '>'; 
}

nav li > a:only-child:after 
{ 
	content: ''; 
}

nav ul ul 
{
	display: none;
	position: absolute; 
	top: 35px;
	z-index: 10;
}
	
nav ul li:hover > ul 
{
	display:list-item;
}
	
nav ul ul li 
{
	width:200px;
	float:none;
	position: relative;
	border-bottom:none;
}

nav ul ul ul li 
{
	position: absolute relative;
	top:-35px; 
	left:200px;
}


